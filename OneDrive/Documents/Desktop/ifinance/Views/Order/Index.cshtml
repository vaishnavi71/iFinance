@model List<Order>
 
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Order Date</th>
            @*<th>Customer</th>*@
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.OrderID</td>
                <td>@customer.OrderDate</td>
                @*<td>@customer.Customer.CustomerName</td>*@
                <td>
                    <a asp-action="Edit" asp-route-id="@customer.OrderID">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@customer.OrderID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
 
<a asp-action="Create">Create</a>