@model S5G11_PVFAPP.Models.RawMaterial

<h2>Edit Raw Material</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="MaterialID" class="control-label"></label>
        <input asp-for="MaterialID" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="MaterialName" class="control-label"></label>
        <input asp-for="MaterialName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaterialStandardCost" class="control-label"></label>
        <input asp-for="MaterialStandardCost" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UnitOfMeasure" class="control-label"></label>
        <input asp-for="UnitOfMeasure" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>