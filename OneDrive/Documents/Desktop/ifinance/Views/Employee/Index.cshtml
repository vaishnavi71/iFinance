@model List<Employee>
 
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.EmployeeId</td>
                <td>@customer.EmployeeName</td>
                <td>@customer.EmployeeAddress</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@customer.EmployeeId">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@customer.EmployeeId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
 
<a asp-action="Create">Create</a>